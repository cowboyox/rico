{
	"extends": ["react-app"],
	"parser": "babel-eslint",
	"plugins": ["react-hooks"],
	"rules": {
		"react-hooks/rules-of-hooks": "error",
		"react-hooks/exhaustive-deps": "off",
		"no-delete-var": "error",
		"no-dupe-args": "error",
		"no-dupe-class-members": "error",
		"no-dupe-keys": "error",
		"no-duplicate-case": "error"
	},
	"env": {
		"es2021": true,
		"node": true
	}
}
